source "https://rubygems.org"

# This gem automatically provides: 
# jekyll-paginate, jekyll-sitemap, jekyll-gist, jekyll-feed, jemoji
gem "github-pages", group: :jekyll_plugins

# This is required for Minimal Mistakes and is NOT in the default github-pages gem
gem "jekyll-include-cache", group: :jekyll_plugins

gem "tzinfo-data"
gem "wdm", "~> 0.1.0" if Gem.win_platform?

# Non-GitHub supported plugins go here
group :algolia_search do
  gem "jekyll-algolia"
end
